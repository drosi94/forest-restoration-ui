{
    "name": "@forest-restoration/pwa",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "doppler run -- next dev",
        "dev:e2e": "doppler run --config dev_e2e -- next dev",
        "dev:env": "next dev",
        "build": "next build",
        "build:doppler": "doppler run -- next build",
        "start": "doppler run -- next start",
        "start:https": "doppler run -- node server.js",
        "start:https:env": "node server.js",
        "cypress:headless": "cypress run",
        "cypress:headless:ci": "cypress run --record",
        "test": "jest",
        "test:u": "jest -u",
        "test:ci": "jest --ci",
        "test:ci:u": "jest --ci -u",
        "test:e2e": "concurrently \"yarn dev:e2e\" \"wait-on http-get://localhost:3000 && doppler run --config dev_e2e -- cypress run\" ",
        "test:e2e:ci": "cd ../firebase && yarn build:functions && firebase emulators:exec --project forest-restoration-staging --only firestore,auth,functions 'cd ../pwa && start-server-and-test dev:env http://localhost:3000 cypress:headless:ci' --import=scripts/firebase/firestore-export/ --export-on-exit",
        "test:e2e:open": "concurrently \"yarn dev:e2e\" \"wait-on http-get://localhost:3000 && doppler run --config dev_e2e -- cypress open\" ",
        "lint": "next lint"
    },
    "dependencies": {
        "@emotion/css": "^11.1.3",
        "@emotion/react": "^11.4.1",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "11.3.0",
        "@forest-restoration/shared": "0.0.0",
        "@heroicons/react": "^1.0.4",
        "@sanity/block-content-to-react": "^3.0.0",
        "@types/react": "^17.0.18",
        "date-fns": "^2.24.0",
        "dotenv": "^10.0.0",
        "firebase": "^8.10.0",
        "firebaseui": "^5.0.0",
        "framer-motion": "^5.2.1",
        "loadash": "^1.0.0",
        "logrocket": "^2.0.0",
        "next": "^12.0.3",
        "next-compose-plugins": "^2.2.1",
        "next-i18next": "^10.2.0",
        "next-images": "^1.8.1",
        "next-pwa": "^5.4.0",
        "next-sanity": "^0.4.0",
        "next-sanity-image": "^3.1.6",
        "next-transpile-modules": "^9.0.0",
        "next-use-contextual-routing": "^2.1.0",
        "nextjs-breadcrumbs": "^1.1.9",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-firebase-hooks": "^3.0.4",
        "react-hook-form": "^7.16.0",
        "react-hot-toast": "^2.1.0",
        "react-tracked": "^1.7.4",
        "react-use": "^17.2.4",
        "typescript": "^4.3.5"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-react": "^7.14.5",
        "@emotion/babel-plugin": "11.3.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@types/jest": "^27.0.1",
        "babel-jest": "^27.0.6",
        "babel-plugin-macros": "^3.1.0",
        "cypress": "^8.4.1",
        "cypress-firebase": "^2.0.0",
        "eslint": "8.2.0",
        "eslint-config-next": "12.0.3",
        "eslint-plugin-cypress": "^2.12.1",
        "firebase-admin": "^9.8.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.0.6",
        "jest-watch-typeahead": "^1.0.0",
        "react-test-renderer": "^17.0.2",
        "start-server-and-test": "^1.14.0"
    }
}